<%- if controller_name != 'sessions' && controller_name != 'passwords' %>
  <%= link_to "Entrar", new_session_path(resource_name), style: 'color:#000515; font-weight: bold;' %><br />
<% end -%>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Nova inscrição", new_registration_path(resource_name), style: 'color:#0C788E;' %><br />
<% end -%>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%= link_to "Esqueceu sua senha?", new_password_path(resource_name), style: 'color:#000515;' %><br />
<% end -%>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <%= link_to "Não recebeu as instruções de confirmação?", new_user_confirmation_path(resource_name), style: 'color:#000515;' %><br />
<% end -%>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <%= link_to "Não recebeu as instruções de ativação?", new_unlock_path(resource_name), style: 'color:#000515;' %><br />
<% end -%>

